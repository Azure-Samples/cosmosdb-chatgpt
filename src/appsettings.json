{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CosmosDb": {
    "Endpoint": "<cosmos-db-nosql-endpoint>",
    "Database": "ChatDatabase",
    "ChatContainer": "ChatContainer",
    "CacheContainer": "CacheContainer"
  },
  "OpenAi": {
    "Endpoint": "<azure-openai-endpoint>",
    "CompletionDeploymentName": "<completion-model-name>",
    "EmbeddingDeploymentName": "<embedding-model-name>"
  },
  "SemanticKernel": {
    "Endpoint": "<azure-openai-endpoint>",
    "CompletionDeploymentName": "<completion-model-name>",
    "EmbeddingDeploymentName": "<embedding-model-name>"
  },
  "Chat": {
    "MaxConversationTokens": "100",
    "CacheSimilarityScore": "0.99"
  }
}